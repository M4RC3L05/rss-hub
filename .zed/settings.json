{
  "deno": {
    "enable": true
  },
  "format_on_save": "on",
  "languages": {
    "TypeScript": {
      "formatter": "language_server",
      "language_servers": [
        "deno",
        "!eslint",
        "!typescript-language-server",
        "!vtsls",
        "..."
      ]
    },
    "TSX": {
      "formatter": "language_server",
      "language_servers": [
        "deno",
        "!eslint",
        "!typescript-language-server",
        "!vtsls",
        "..."
      ]
    }
  }
}
