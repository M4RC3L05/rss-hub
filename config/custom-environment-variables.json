{
  "apps": {
    "api": {
      "host": "APPS_API_HOST",
      "port": {
        "__name": "APPS_API_PORT",
        "__format": "number"
      },
      "basicAuth": {
        "username": "APPS_API_BASIC_AUTH_USERNAME",
        "password": "APPS_API_BASIC_AUTH_PASSWORD"
      }
    },
    "web": {
      "host": "APPS_WEB_HOST",
      "port": {
        "__name": "APPS_WEB_PORT",
        "__format": "number"
      },
      "basicAuth": {
        "username": "APPS_WEB_BASIC_AUTH_USERNAME",
        "password": "APPS_WEB_BASIC_AUTH_PASSWORD"
      },
      "services": {
        "api": {
          "url": "APPS_WEB_SERVICES_API_URL",
          "basicAuth": {
            "username": "APPS_WEB_SERVICES_API_BASIC_AUTH_USERNAME",
            "password": "APPS_WEB_SERVICES_API_BASIC_AUTH_PASSWORD"
          }
        }
      }
    },
    "feeds-synchronizer": {
      "cron": {
        "timezone": "FEEDS_SYNCHRONIZER_CRON_TIMEZONE",
        "pattern": "FEEDS_SYNCHRONIZER_CRON_PATTERN",
        "tickerTimeout": {
          "__name": "FEEDS_SYNCHRONIZER_CRON_TICKER_TIMEOUT",
          "__format": "number"
        }
      }
    }
  },
  "database": {
    "path": "DATABASE_PATH"
  }
}
