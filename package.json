{
  "name": "rsshub",
  "version": "4.19.3",
  "description": "",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "imports": {},
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "lint": "biome lint . && tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint-staged": "lint-staged",
    "dev:api": "node --watch --no-warnings --loader ts-node/esm src/apps/api/main.ts | pino-pretty",
    "dev:web": "node --watch --no-warnings --loader ts-node/esm src/apps/web/main.ts | pino-pretty",
    "dev:feeds-synchronizer": "node --watch --no-warnings --loader ts-node/esm src/apps/feeds-synchronizer/main.ts | pino-pretty",
    "db:reset": "rm -rf ./data/app.db && npm run db:migrate",
    "db:seed": "sqlite3 ./data/app.db \".read ./src/database/seed.sql\"",
    "db:migrate": "dbmate -u 'sqlite:./data/app.db' -d ./src/database/migrations --no-dump-schema migrate",
    "version": "offline-github-changelog --next=${npm_new_version} | sed -e '/Release v.*/d;s/ (\\[.*\\]\\(.*\\))//gm' > CHANGELOG.md && git add CHANGELOG.md",
    "postinstall": "patch-package || :"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@types/better-sqlite3": "7.6.8",
    "@types/config": "3.3.3",
    "@types/lodash-es": "4.17.12",
    "@types/masonry-layout": "4.2.7",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.10.5",
    "@types/react-dom": "18.2.18",
    "dbmate": "2.9.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "masonry-layout": "4.2.2",
    "offline-github-changelog": "3.0.1",
    "patch-package": "8.0.0",
    "pino-pretty": "10.3.0",
    "react": "18.2.0",
    "react-bootstrap": "2.9.1",
    "react-dom": "18.2.0",
    "swr": "2.2.4",
    "typescript": "5.3.3",
    "usehooks-ts": "2.9.1"
  },
  "dependencies": {
    "@hono/node-server": "1.3.3",
    "@hono/zod-validator": "0.1.11",
    "@leafac/sqlite": "4.0.0",
    "better-sqlite3": "9.2.2",
    "config": "3.3.9",
    "cron-parser": "4.9.0",
    "entities": "4.5.0",
    "fast-xml-parser": "4.3.2",
    "hono": "3.11.8",
    "lodash-es": "4.17.21",
    "mime-types": "2.1.35",
    "node-fetch": "3.3.2",
    "node-html-parser": "6.1.11",
    "pino": "8.17.1",
    "ts-node": "10.9.2",
    "zod": "3.22.4"
  }
}
