{
  "name": "rsshub",
  "version": "4.12.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "lint": "xo && tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint-staged": "lint-staged",
    "dev:api": "node --watch --no-warnings --loader ts-node/esm src/apps/api/main.ts | pino-pretty",
    "dev:web": "node --watch --no-warnings --loader ts-node/esm src/apps/web/main.ts | pino-pretty",
    "dev:feeds-synchronizer": "node --watch --no-warnings --loader ts-node/esm src/apps/feeds-synchronizer/main.ts | pino-pretty",
    "db:reset": "rm -rf ./data/app.db && npm run db:migrate",
    "db:seed": "sqlite3 ./data/app.db \".read ./src/database/seed.sql\"",
    "db:migrate": "dbmate -u 'sqlite:./data/app.db' -d ./src/database/migrations -s ./src/database/schema.sql --no-dump-schema migrate",
    "version": "offline-github-changelog --next=${npm_new_version} | sed -e '/Release v.*/d;s/ (\\[.*\\]\\(.*\\))//gm' > CHANGELOG.md && git add CHANGELOG.md",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": "18.x.x"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "editorconfig": true,
    "proseWrap": "preserve",
    "printWidth": 100,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "xo": {
    "env": [
      "node",
      "browser"
    ],
    "prettier": true,
    "overrides": [
      {
        "files": "**/public/**/*.ts",
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "@typescript-eslint/naming-convention": "off"
        }
      }
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@types/better-sqlite3": "^7.6.6",
    "@types/config": "^3.3.2",
    "@types/koa": "^2.13.10",
    "@types/koa__cors": "^4.0.2",
    "@types/koa__router": "^12.0.2",
    "@types/koa-basic-auth": "^2.0.5",
    "@types/koa-bodyparser": "^4.3.11",
    "@types/koa-qs": "^2.0.2",
    "@types/koa-static": "^4.0.3",
    "@types/lodash-es": "^4.17.10",
    "@types/masonry-layout": "^4.2.6",
    "@types/mime-types": "^2.1.3",
    "@types/node": "^20.8.7",
    "@types/react-dom": "^18.2.14",
    "dbmate": "^2.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "masonry-layout": "^4.2.2",
    "offline-github-changelog": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.4",
    "typescript": "^5.2.2",
    "usehooks-ts": "^2.9.1",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "@fastify/busboy": "^2.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@leafac/sqlite": "^4.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "better-sqlite3": "^9.0.0",
    "config": "^3.3.9",
    "cron-parser": "^4.9.0",
    "entities": "^4.5.0",
    "fast-xml-parser": "^4.3.2",
    "http-errors": "^2.0.0",
    "json-schema-to-ts": "^2.9.2",
    "koa": "^2.14.2",
    "koa-basic-auth": "^4.0.0",
    "koa-bodyparser": "^4.4.1",
    "koa-proxies": "^0.12.3",
    "koa-qs": "^3.0.0",
    "koa-static": "^5.0.0",
    "lodash-es": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.10",
    "patch-package": "^8.0.0",
    "pino": "^8.16.0",
    "ts-node": "^11.0.0-beta.1"
  }
}
